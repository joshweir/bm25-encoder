{
  "extends": ["maddis-code-rules/tslint"],
  "rules": {
    "no-unused-expression": true,
    "no-floating-promises": true,
    "no-function-constructor-with-string-args": false,
    "await-promise": [true, "Bluebird", "IRestrictedQueryBuilder", "PromiseLike"],
    "import-name": [
      true,
      {
        "react": "React",
        "reactDom": "ReactDOM",
        "nimblePicker": "NimblePicker",
        "dataloader": "DataLoader",
        "awsSdk": "AWS",
        "s3": "S3",
        "sns": "SNS",
        "sqs": "SQS",
        "sts": "STS",
        "ssm": "SSM",
        "lambda": "Lambda",
        "kinesis": "Kinesis",
        "dynamodb": "DynamoDB",
        "cloudsearch": "CloudSearch",
        "cloudwatchlogs": "CloudWatchLogs",
        "cloudwatchevents": "CloudWatchEvents",
        "cloudsearchdomain": "CloudSearchDomain",
        "apigatewaymanagementapi": "ApiGatewayManagementApi",
        "cognitoidentityserviceprovider": "CognitoIdentityServiceProvider",
        "knex": "client",
        "difference_in_seconds": "differenceInSeconds",
        "difference_in_hours": "differenceInHours",
        "distance_in_words": "distanceInWords",
        "start_of_tomorrow": "startOfTomorrow",
        "start_of_week": "startOfWeek",
        "is_same_year": "isSameYear",
        "is_same_day": "isSameDay",
        "add_hours": "addHours",
        "set_month": "setMonth",
        "add_days": "addDays",
        "set_date": "setDate",
        "chance": "Chance",
        "koa": "Koa",
        "koaRouter": "Router",
        "koaBodyparser": "bodyParser",
        "ws": "Server",
        "commDbMigrationHelpersNode": "helper",
        "commConversationAccessValidatorNode": "validator",
        "commSmokeballPermissionsNode": "smokeballPermissions",
        "entitiesFromTypes": "allEntitiesImported",
        "itopsNodeFetchWrapperNode": "nodeFetch",
        "icalJs": "ICAL"
      }
    ]
  }
}
